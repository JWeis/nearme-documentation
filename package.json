{
  "name": "desksnearme",
  "version": "1.0.0",
  "description": "Documentation for NearMe Marketplace Platform.",
  "private": true,
  "directories": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdyd-dev/nearme-documentation.git"
  },
  "scripts": {
    "start": "npm run serve & npm run assets",
    "assets": "webpack -w --env development",
    "serve": "bundle exec jekyll serve -w --incremental",
    "build": "webpack -p --env production",
    "lint:js": "eslint src/ webpack.*.js --cache",
    "lint:style": "stylelint src/scss/**/*.scss",
    "lint:html":
      "bundle exec htmlproofer ./_site --http-status-ignore '999' --check-html --assume-extension --check-opengraph --check-favicon",
    "lint": "npm run lint:js; npm run lint:style; npm run lint:html"
  },
  "pre-commit": ["build"],
  "author": "NearMe",
  "license": "UNLICENSED",
  "homepage": "https://github.com/mdyd-dev/nearme-documentation",
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-preset-es2015": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "cache-loader": "^1.1.0",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.2.0",
    "css-loader": "^0.28.7",
    "cssnano": "^3.10.0",
    "eslint": "^4.9.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "flow-bin": "^0.57.3",
    "flow-typed": "^2.2.1",
    "html-webpack-harddisk-plugin": "^0.1.0",
    "html-webpack-inline-chunk-plugin": "^1.1.1",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss-loader": "^2.0.8",
    "pre-commit": "^1.2.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "stylelint": "^8.2.0",
    "webpack": "^3.8.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-beta.2",
    "hammerjs": "^2.0.8",
    "normalize-scss": "^7.0.0",
    "prismjs": "^1.8.3"
  }
}
